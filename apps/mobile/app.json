{
  "expo": {
    "name": "Rise",
    "slug": "riseremote",
    "scheme": "rise",
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./assets/RiseIcon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/RiseSplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#260162"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/e375003a-c8cd-4eec-bec8-202333e67dc6"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "link.verse.rise",
      "buildNumber": "6"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/RiseIcon-Adaptive.png",
        "backgroundColor": "#260162",
        "monochromeImage": "./assets/RiseIcon-Adaptive-Mono.png"
      },
      "package": "com.xplatlabs.rise",
      "versionCode": 4
    },
    "androidStatusBar": {
      "backgroundColor": "#260162",
      "barStyle": "light-content"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d8f030d6-44d4-44cd-bec1-21d303f650b3"
      }
    },
    "owner": "ericvicenti",
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
